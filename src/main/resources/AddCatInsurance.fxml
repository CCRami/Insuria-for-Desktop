<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="720.0" prefWidth="1030.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddCatInsurance">
    <children>
        <BorderPane fx:id="borderPane" prefHeight="720.0" prefWidth="1010.0">


            <center>
                <VBox fx:id="vboxdash" prefHeight="720.0" prefWidth="1013.0">
                    <children>
                        <!-- Add a new insurance form -->
                        <HBox prefHeight="643.0" prefWidth="980.0" styleClass="show-box" stylesheets="@style/styleafficher.css">
                            <!-- Insurance details input fields -->
                            <VBox fx:id="fieldsContainer" prefHeight="550.0" prefWidth="316.0">
                                <!-- Name of insurance -->


                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Insurance Category Name" wrappingWidth="141.33674240112305">
                                    <VBox.margin>
                                        <Insets left="77.0" />
                                    </VBox.margin>
                                </Text>
                                <TextField fx:id="CatinsuranceNameField" prefHeight="34.0" prefWidth="229.0" promptText="Enter Category name" stylesheets="@style/styleafficher.css">
                                    <VBox.margin>
                                        <Insets left="70.0" right="50.0" top="10.0" />
                                    </VBox.margin>
                                    <styleClass>
                                        <String fx:value="search" />
                                        <String fx:value="shadow" />
                                    </styleClass>
                                </TextField>
      
                                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Insurance Category Description" wrappingWidth="184.5367546081543">
                                    <VBox.margin>
                                        <Insets left="77.0" />
                                    </VBox.margin>
                                </Text>
                                <TextField fx:id="desccatinsField" prefHeight="152.0" prefWidth="219.0" promptText="Enter insurance Category Description" stylesheets="@style/styleafficher.css">
                                    <VBox.margin>
                                        <Insets left="70.0" top="200.0" />
                                    </VBox.margin>
                                    <styleClass>
                                        <String fx:value="search" />
                                        <String fx:value="shadow" />
                                    </styleClass>
                                </TextField>

                            </VBox>
                            <VBox prefHeight="598.0" prefWidth="374.0">

                                <Button fx:id="save" mnemonicParsing="false" onAction="#addcatinsAction" prefHeight="43.0" prefWidth="103.0" styleClass="search-button" stylesheets="@style/styleafficher.css" text="Enregistrer" textFill="WHITE">
                                    <VBox.margin>
                                        <Insets left="150.0" top="160.0" />
                                    </VBox.margin>
                                    <font>
                                        <Font size="13.0" />
                                    </font>
                                </Button>
                            </VBox>

                        </HBox>
                    </children>
                </VBox>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
