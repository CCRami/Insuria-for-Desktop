<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="778.0" prefWidth="1254.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ShowInsurance">
    <children>
        <!-- TableView to display insurance details -->
        <AnchorPane layoutX="433.0" layoutY="176.0" prefHeight="598.0" prefWidth="808.0" stylesheets="@style/dashboardDesign.css">
            <TableView fx:id="insuranceTableView" layoutX="11.0" layoutY="61.0" prefHeight="530.0" prefWidth="786.0">
                <columns>
                    <TableColumn fx:id="nameins" prefWidth="185.60003662109375" text="Insurance Name" />
                    <TableColumn fx:id="amount" prefWidth="100" text="Amount" />
                    <TableColumn fx:id="insimage" prefWidth="150" text="Image" />
                    <TableColumn fx:id="catinsid" prefWidth="150" text="Categories" />
                    <TableColumn fx:id="polid" prefWidth="150" text="Police" />
                </columns>
            </TableView>
            <!-- Search TextField -->
            <TextField fx:id="searchTextField" layoutX="14.0" layoutY="14.0" prefHeight="30.0" prefWidth="277.0" promptText="Search" styleClass="search" stylesheets="@style/dashboardDesign.css" />
        </AnchorPane>

        <!-- Insurance details input fields and buttons -->
        <AnchorPane layoutX="39.0" layoutY="176.0" prefHeight="598.0" prefWidth="378.0" stylesheets="@style/dashboardDesign.css">
            <TextField fx:id="insuranceNameField" layoutX="126.0" layoutY="63.0" prefHeight="26.0" prefWidth="207.0" styleClass="search" stylesheets="@style/dashboardDesign.css" />
            <TextField fx:id="insuranceAmountField" layoutX="126.0" layoutY="131.0" prefHeight="26.0" prefWidth="207.0" styleClass="search" stylesheets="@style/dashboardDesign.css" />
            <TextField fx:id="insuranceImageField" layoutX="125.0" layoutY="260.0" prefHeight="26.0" prefWidth="207.0" styleClass="search" stylesheets="@style/dashboardDesign.css" />
            <TextField fx:id="insuranceCategoriesField" layoutX="126.0" layoutY="195.0" prefHeight="26.0" prefWidth="207.0" styleClass="search" stylesheets="@style/dashboardDesign.css" />
            <TextField fx:id="insurancePoliceField" layoutX="126.0" layoutY="320.0" prefHeight="26.0" prefWidth="207.0" styleClass="search" stylesheets="@style/dashboardDesign.css" />

            <Text layoutX="57.0" layoutY="81.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Insurance Name" />
            <Text layoutX="49.0" layoutY="149.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Amount" />
            <Text layoutX="54.0" layoutY="213.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Image" />
            <Text layoutX="51.0" layoutY="278.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Categories" />
            <Text layoutX="48.0" layoutY="338.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Police" />

            <!-- Error messages -->
            <Text fx:id="errorInsuranceName" fill="RED" layoutX="130.0" layoutY="115.0" wrappingWidth="181.5999755859375" />
            <Text fx:id="errorInsuranceAmount" fill="RED" layoutX="130.0" layoutY="180.0" wrappingWidth="195.20001220703125" />
            <Text fx:id="errorInsuranceImage" fill="RED" layoutX="125.0" layoutY="245.0" wrappingWidth="190.4000244140625" />
            <Text fx:id="errorInsuranceCategories" fill="RED" layoutX="130.0" layoutY="308.0" wrappingWidth="195.20001220703136" />
            <Text fx:id="errorInsurancePolice" fill="RED" layoutX="130.0" layoutY="370.0" wrappingWidth="195.20001220703136" />
        </AnchorPane>
    </children>
</AnchorPane>
